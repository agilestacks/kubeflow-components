apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: "${hub.componentName}"
  namespace: "${component.dex.namespace}"
spec:
  rules:
    - host: "${component.dex.oidcIssuerFqdn}"
      http:
        paths:
          - backend:
              service:
                name: dex
                port:
                  number: 5556
            path: /
            pathType: Prefix
